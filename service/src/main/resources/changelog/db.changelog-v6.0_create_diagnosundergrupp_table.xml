<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd"
        logicalFilePath="liquibase/db.changelog-v6.0_create_diagnosundergrupp_table.xml">
    <changeSet id="6a_create_diagnosis_sub_group_data_table" author="roger.lindsjo">
        <createTable tableName="diagnosundergrupp">
            <column name="period" type="varchar(20)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="hsaid" type="varchar(60)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="typ" type="varchar(60)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="diagnosgrupp" type="varchar(20)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="undergrupp" type="varchar(20)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="female" type="int"/>
            <column name="male" type="int"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
