<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd"
                   logicalFilePath="liquibase/db.changelog-add_lan_and_kommun_to_enhet_table.xml">
    <changeSet id="18_add_land_and_kommun_to_enhet_table" author="jesper.holmberg">
        <addColumn tableName="enhet">
            <column name="lansId" type="varchar(2)">
                <constraints nullable="false"/>
            </column>
            <column name="kommunId" type="varchar(4)">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
