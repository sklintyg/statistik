<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd"
                   logicalFilePath="liquibase/db.changelog-v15.0_add_intygtype_column_to_wideline.xml">
    <changeSet id="15a_add_intygtyp_and_correlationid_to_wideline_table" author="pierre.sandboge">
        <addColumn tableName="wideline">
            <column name="intygtyp" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="correlationId" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>