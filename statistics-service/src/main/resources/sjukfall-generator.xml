<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-placeholder/>
    <context:annotation-config/>

    <bean class="org.springframework.core.io.ClassPathResource" id="icd10KapitelAnsiFile" >
        <constructor-arg value="icd10/KSH97_KAP.ANS"/>
    </bean>
    <bean class="org.springframework.core.io.ClassPathResource" id="icd10AvsnittAnsiFile" >
        <constructor-arg value="icd10/KSH97_AVS.ANS"/>
    </bean>
    <bean class="org.springframework.core.io.ClassPathResource" id="icd10KategoriAnsiFile" >
        <constructor-arg value="icd10/KSH97_KAT.ANS"/>
    </bean>
    
    <bean class="se.inera.statistics.service.report.util.Icd10" />

    <bean class="se.inera.statistics.service.demo.SjukfallGenerator"/>
    <bean class="se.inera.statistics.service.demo.SjukfallGenerator$PrintConsumer"/>
</beans>
