{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "nonbsp": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxstatements": 150,
    "maxcomplexity": 15,
    "maxlen": 350,
    "maxerr": 10000,
    "browser": false,
    "devel": false,
    "esversion": 5,

    "globals": {
        "afterEach": false,
        "beforeEach": false,
        "describe": false,
        "expect": false,
        "it": false,
        "xit": false,
        "xdescribe": false,
        "spyOn": false,
        "jasmine": false,
        "angular": true,
        "moment": false,
        "$": true,
        "module": true,
        "inject": true,
        "MODULE_CONFIG": true,
        "document": true,
        "deferredBootstrapper": true,
        "navigator": true
    }
}
